package operator;

public class OperatorEx1java {

	public static void main(String[] args) {
		// 변수값 교환
		int x = 0;
		int y = 1;
		
		System.out.println("***** 교환 전 *****");
		System.out.println("x = " + x + ", y = " + y);
		
		//연산(교환)
		int temp = x;
		temp = x;
		x = y;
		y = temp;
		
		System.out.println("***** 교환 후 *****");
		System.out.println("x = " + x + ", y = " + y);
		
		
		
		

	}

}
