package choice;

/*
 *  학점 계산 프로그램
 *  90~100점 : A
 *  80~89점 : B
 *  70~79점 : C
 *  70점 미만 : F
 *  
 */

public class IfElse {

	public static void main(String[] args) {
		//다중 조건문
		int score = 86;
		char grade = ' ';    //문자를 저장할 변수 grade 선언, 공백문자
		
		if(score >= 90 && score <= 100) {
			grade = 'A';
		}else if(score >= 80) {
			grade = 'B';
		}else if(score >= 70) {
			grade = 'C';
		}else {
			grade = 'F';
		}

		System.out.println("학점은 " + grade + "입니다.");
	}
}
